{
  "manifest_version": 3,
  "name": "DeepSeek Exporter",
  "version": "1.0",
  "permissions": ["activeTab", "scripting", "downloads"],
  "content_scripts": [{
    "matches": ["*://*.deepseek.com/*"],
    "css": ["chrome_extension/content.css"],
    "js": ["chrome_extension/content.js"]
  }],
  "background": {
    "service_worker": "chrome_extension/background.js"
  }
}
