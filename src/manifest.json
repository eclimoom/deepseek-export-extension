{
  "manifest_version": 3,
  "name": "DeepSeek截图 DeepSeek导出图片",
  "description": "DeepSeek,DeepSeek截图, DeepSeek导出图片, DeepSeek缩放字号, 保存为图片",

  "version": "1.0.6",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [{
    "matches": ["https://chat.deepseek.com/*"],
    "css": ["chrome_extension/content.css"],
    "js": ["chrome_extension/html2canvas.min.js", "chrome_extension/pdf-lib.min.js","chrome_extension/content.js"]
  }],
  "icons": {
    "16": "assets/icons/16.png",
    "48": "assets/icons/48.png",
    "128": "assets/icons/128.png"
  }
}
